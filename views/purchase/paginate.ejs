<!--paginate.ejs-->
<table>
    <% purchases.forEach( function(model) { %>
        <tr><td><%=model.firstname%></td><td><%=model.surname%></td><td><%=model.age%><td><%=model.sex%></td><td><%=model.email%></td><td><%=model.region%></td><td><%=model.city%></td></td><td><%=model.class%></td><td><%=model.price%></td><td><%=model.purchasedate%></td><td><%=model.amount></td><td><%=model.tourdates></td></tr>
    <% }); %>
</table>

<% var current = req.query.page %>

<nav>
    <ul class="pagination">
